<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.heike.dao.DeleteDao">
<delete id="deleteH" parameterType="Integer">
		delete from t_house where house_id=#{house_id}
	</delete>
	<delete id="deleteT" parameterType="Integer">
		delete from t_teacher where t_id=#{t_id}
	</delete>
	<delete id="deleteS" parameterType="Integer">
		delete from t_stu where stu_id=#{stu_id}
	</delete>
	<delete id="refuseS" parameterType="Integer">
		delete from t_stu where stu_id=#{s_id}
	</delete>
	<delete id="deleteTask" parameterType="Integer">
		delete from t_task where task_id=#{task_id}
	</delete>
</mapper> 